@model IEnumerable<cshh.Data.Viber.ViberEvent>

@{
    ViewBag.Title = "Viber events";
}

<h2>View</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateTimeUTC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestContent)
        </th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateTimeUTC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestContent)
        </td>                        
    </tr>
}

</table>
